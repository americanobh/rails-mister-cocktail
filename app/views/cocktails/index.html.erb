        <div class="container mt-3">
      <div class="row">
        <div class="col-12">
          <h1><%= 'All our cocktails'%> (<%= Cocktail.count %>)</h1>
        </div>
      </div>

            <div class="row">
        <div class="col">
            <p>All cocktails.</p>
        </div>
      </div>




  <div class="flexbox">

      <div class="row">

<% @cocktails.each do |cocktail|  %>
          <div class="col-4">
            <div class="flat-card">
              <div class="flat-card-image" style="background-image: url('data:image/jpeg;base64,')"></div>
              <div class="flat-card-content">
                <div class="flat-card-title">
                  <h4><%= link_to cocktail.name, cocktail_path(cocktail) %></h4>
                </div>
                <p><%= cocktail.name %></p>
                <% cocktail.doses.each do |dosy| %>
                 <div class="flex">
                  <div class="flex">
                    <i class="material-icons mr-2">people</i>
                    <span>
                      <%= dosy.ingredient.name %>
                    </span>
                  </div>
                  <br>
                  <div class="flex ml-3">
                    <i class="material-icons mr-2">euro_symbol</i><%= dosy.description %>
                  </div>
                  </div>
                  <br>
                  <% end %>

              </div>
              <div class="flat-card-actions">
                <a href="/cocktails/<%= cocktail.id %>">More info</a>
                <a class="ml-3 text-warning" href="/cocktails/<%= cocktail.id %>/edit">Edit</a>
                <a data-confirm="Are you sure?" class="ml-3 text-danger" rel="nofollow" data-method="delete" href="/cocktails/<%= cocktail.id %>">Remove</a>
              </div>
            </div>
          </div>

<% end %>
